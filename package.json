{
  "name": "gambiconf",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && touch ./build/.nojekyll && echo \"gambiconf.dev\" > ./build/CNAME",
    "build:prod": "export ENV=production && npm run build && cp build/index.html build/404.html",
    "build:staging": "export ENV=staging && npm run build && cp build/index.html build/404.html",
    "deploy:prod": "gh-pages --repo git@github.com:gambiconf/gambiconf.github.io.git -d build -t true",
    "deploy:staging": "gh-pages --repo git@github.com:gambiconf/website-staging.git -d build -t true",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check . && eslint .",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "@fluent/langneg": "^0.7.0",
    "@fontsource/ibm-plex-sans": "^5.2.5",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "dayjs": "1.11.13",
    "flag-icons": "^7.3.2",
    "jsdom": "^25.0.1",
    "query-string": "9.1.1",
    "rough-notation": "^0.5.1",
    "svelte-fa": "^4.0.4",
    "svelte-flag-icons": "^2.0.1",
    "svelte-loading-spinners": "^0.3.6",
    "svelte-rough-notation": "^0.1.5",
    "svelte-select": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@nubolab-ffwd/svelte-fluent": "^1.0.3",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "svelte-eslint-parser": "^1.1.3",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2"
  },
  "type": "module"
}
